<%- contentFor('body') %>
  <h2>Professor <%= data.name+ " " + data.surname%></h2>
  <table border="1">
    <thead>
      <tr>
        <td>Nome</td>
        <td>Sobrenome</td>
        <td>Ação</td>
      </tr>
    </thead>
    <tbody> 
        <tr>
          <td>
            <%= data.name %>
          </td>
          <td>
            <%= data.surname %>
          </td>
          <td><a href="/teacher/edit/<%= data._id %>">Editar</a> - <a href="/teacher/delete/<%= data._id %>">Deletar</a></td>
        </tr>
    </tbody>
  </table>
  <button><a href="/teacher/show">Voltar</a></button>